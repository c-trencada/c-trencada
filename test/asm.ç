#inclou "test.cç"

caràcter *funció_1(buit) {
  asm("mov $50, %rax\n\t"
      "mov %rbp, %rsp\n\t"
      "pop %rbp\n\t"
      "ret");
}

caràcter *funció_2(buit) {
  asm en_línia volàtil("mov $55, %rax\n\t"
                      "mov %rbp, %rsp\n\t"
                      "pop %rbp\n\t"
                      "ret");
}

enter principal() {
  AFIRMA(50, funció_1());
  AFIRMA(55, funció_2());

  imprimeix("OK\n");
  retorna 0;
}
