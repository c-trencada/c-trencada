#inclou "test.cç"

extern enter ext1;
extern enter *ext2;

en_línia enter funció_en_línia(buit) {
  retorna 3;
}

enter principal() {
  AFIRMA(5, ext1);
  AFIRMA(5, *ext2);

  extern enter ext3;
  AFIRMA(7, ext3);

  enter funció_externa_1(enter valor);
  AFIRMA(5, funció_externa_1(5));

  extern enter funció_externa_2(enter valor);
  AFIRMA(8, funció_externa_2(8));

  imprimeix("OK\n");
  retorna 0;
}
