#inclou "test.cç"

ent ret_3(buit) {
  retorna 3;
  retorna 5;
}

ent afegeix_2(ent valor_x, ent valor_y) {
  retorna valor_x + valor_y;
}

ent resta_2(ent valor_x, ent valor_y) {
  retorna valor_x - valor_y;
}

ent afegeix_6(ent valor_a, ent valor_b, ent valor_c, ent valor_d, ent valor_e, ent valor_f) {
  retorna valor_a + valor_b + valor_c + valor_d + valor_e + valor_f;
}

ent afegeix_x(ent *punter_x, ent valor_y) {
  retorna *punter_x + valor_y;
}

ent resta_car(car valor_a, car valor_b, car valor_c) {
  retorna valor_a - valor_b - valor_c;
}

ent Fibonacci(ent nombre) {
  si (nombre<=1)
    retorna 1;
  retorna Fibonacci(nombre-1) + Fibonacci(nombre-2);
}

ent resta_llarg(llarg valor_a, llarg valor_b, llarg valor_c) {
  retorna valor_a - valor_b - valor_c;
}

ent sub_short(curt valor_a, curt valor_b, curt valor_c) {
  retorna valor_a - valor_b - valor_c;
}

ent g1;

ent *g1_punter(buit) { retorna &g1; }
car enter_a_car(ent valor) { retorna valor; }

ent div_llarg(llarg dividend, llarg divisor) {
  retorna dividend / divisor;
}

_Bool prova_booleana_suma(_Bool valor) { retorna valor + 1; }
_Bool prova_booleana_resta(_Bool valor) { retorna valor - 1; }

estàtic ent prova_estàtica(buit) { retorna 3; }

ent descomposició_paràmetre(ent taula[]) { retorna taula[0]; }

ent comptador() {
  estàtic ent índex;
  estàtic ent comptador_local = 1+1;
  retorna índex++ + comptador_local++;
}

buit retorna_res() {
  retorna;
}

_Bool retorna_cert();
_Bool retorna_fals();
car retorna_car();
curt retorna_curt();

sense_signe car retorna_ucar();
sense_signe curt retorna_curt_sense_signe();

car retorna_car_signe();
curt retorna_curt_signe();

ent suma_tot(ent nombre, ...);

def_tipus estructura {
  ent desplacament_registre_general;
  ent desplacament_registre_flotant;
  buit *area_arguments_desbordament;
  buit *area_desat_registres;
} __element_arguments_vars;

def_tipus __element_arguments_vars llista_arguments_vars[1];

ent imprimeix_cadena(car *buffer, car *format, ...);
ent imprimeix_cadena_va(car *buffer, car *format, llista_arguments_vars arguments);

car *format(car *buffer, car *format_text, ...) {
  llista_arguments_vars arguments;
  *arguments = *(__element_arguments_vars *)__va_area__;
  imprimeix_cadena_va(buffer, format_text, arguments);
}

doble suma_doble(doble valor_x, doble valor_y);
flot suma_flotant(flot valor_x, flot valor_y);

flot suma_flotant_3(flot valor_x, flot valor_y, flot valor_z) {
  retorna valor_x + valor_y + valor_z;
}

doble suma_doble_precisió_3(doble valor_x, doble valor_y, doble valor_z) {
  retorna valor_x + valor_y + valor_z;
}

ent (*puntera_funció(ent (*invocable)(ent nombre, ...)))(int, ...) {
  retorna invocable;
}

ent descomposició_paràmetre_2(ent invocable()) { retorna invocable(); }

car *funció_nom(buit) {
  retorna __func__;
}

car *funció_nom_complet(buit) {
  retorna __FUNCTION__;
}

ent suma_deu_enter(ent valor_1, ent valor_2, ent valor_3, ent valor_4, ent valor_5, ent valor_6, ent valor_7, ent valor_8, ent valor_9, ent valor_10);
flot suma_deu_reals(flot valor_1, flot valor_2, flot valor_3, flot valor_4, flot valor_5, flot valor_6, flot valor_7, flot valor_8, flot valor_9, flot valor_10);
doble suma_deu_doble(doble valor_1, doble valor_2, doble valor_3, doble valor_4, doble valor_5, doble valor_6, doble valor_7, doble valor_8, doble valor_9, doble valor_10);

ent paràmetres_vars_1(ent arg_a, ent arg_b, ent arg_c, ent arg_d, ent arg_e, ent arg_f, ent arg_g, ent arg_h) {
  retorna arg_g / arg_h;
}

doble paràmetres_vars_2(doble arg_a, doble arg_b, doble arg_c, doble arg_d, doble arg_e,
                  doble arg_f, doble arg_g, doble arg_h, doble arg_i, doble arg_j) {
  retorna arg_i / arg_j;
}

ent paràmetres_vars_3(ent arg_a, doble arg_b, ent arg_c, ent arg_d, doble arg_e, ent arg_f,
               doble arg_g, ent arg_h, doble arg_i, doble arg_j, doble arg_k,
               doble arg_l, doble arg_m, ent arg_n, ent arg_o, doble arg_p) {
  retorna arg_o / arg_p;
}

def_tipus estructura { ent membre_a,membre_b; curt membre_c; car membre_d; } tipus_estructura_4;
def_tipus estructura { ent membre_a; flot membre_b; doble membre_c; } tipus_estructura_5;
def_tipus estructura { sense_signe car membre_a[3]; } tipus_estructura_6;
def_tipus estructura { llarg membre_a, membre_b, membre_c; } tipus_estructura_7;

ent estructura_cas_5(tipus_estructura_5 estructura_valor, ent nombre);
ent estructura_cas_4(tipus_estructura_4 estructura_valor, ent nombre);
ent estructura_cas_6(tipus_estructura_6 estructura_valor, ent nombre);
ent estructura_cas_7(tipus_estructura_7 estructura_valor, ent nombre);

ent estructura_cas_14(tipus_estructura_4 estructura_valor, ent nombre) {
  selecciona (nombre) {
  cas 0: retorna estructura_valor.membre_a;
  cas 1: retorna estructura_valor.membre_b;
  cas 2: retorna estructura_valor.membre_c;
  predeterminat: retorna estructura_valor.membre_d;
  }
}

ent estructura_cas_15(tipus_estructura_5 estructura_valor, ent nombre) {
  selecciona (nombre) {
  cas 0: retorna estructura_valor.membre_a;
  cas 1: retorna estructura_valor.membre_b;
  predeterminat: retorna estructura_valor.membre_c;
  }
}

def_tipus estructura { sense_signe car membre_a[10]; } tipus_estructura_20;
def_tipus estructura { sense_signe car membre_a[20]; } tipus_estructura_21;

tipus_estructura_4 estructura_cas_24(buit);
tipus_estructura_5 estructura_cas_25(buit);
tipus_estructura_6 estructura_cas_26(buit);
tipus_estructura_20 estructura_cas_27(buit);
tipus_estructura_21 estructura_cas_28(buit);

