#inclou "test.c√ß"

enter principal() {
  AFIRMA(1, __predefinit_tipus_compatibles(enter, enter));
  AFIRMA(1, __predefinit_tipus_compatibles(doble, doble));
  AFIRMA(0, __predefinit_tipus_compatibles(enter, llarg));
  AFIRMA(0, __predefinit_tipus_compatibles(llarg, flot));
  AFIRMA(1, __predefinit_tipus_compatibles(enter *, enter *));
  AFIRMA(0, __predefinit_tipus_compatibles(curt *, enter *));
  AFIRMA(0, __predefinit_tipus_compatibles(enter **, enter *));
  AFIRMA(1, __predefinit_tipus_compatibles(const enter, enter));
  AFIRMA(0, __predefinit_tipus_compatibles(sense_signe, enter));
  AFIRMA(1, __predefinit_tipus_compatibles(amb_signe, enter));
  AFIRMA(0, __predefinit_tipus_compatibles(estructura {enter a;}, estructura {enter a;}));

  AFIRMA(1, __predefinit_tipus_compatibles(enter (*)(buit), enter (*)(buit)));
  AFIRMA(1, __predefinit_tipus_compatibles(buit (*)(int), buit (*)(int)));
  AFIRMA(1, __predefinit_tipus_compatibles(buit (*)(int, doble), buit (*)(int, doble)));
  AFIRMA(1, __predefinit_tipus_compatibles(enter (*)(float, doble), enter (*)(float, doble)));
  AFIRMA(0, __predefinit_tipus_compatibles(enter (*)(float, doble), enter));
  AFIRMA(0, __predefinit_tipus_compatibles(enter (*)(float, doble), enter (*)(float)));
  AFIRMA(0, __predefinit_tipus_compatibles(enter (*)(float, doble), enter (*)(float, doble, int)));
  AFIRMA(1, __predefinit_tipus_compatibles(doble (*)(...), doble (*)(...)));
  AFIRMA(0, __predefinit_tipus_compatibles(doble (*)(...), doble (*)(buit)));

  AFIRMA(1, ({ def_tipus estructura {enter a;} Tipus; __predefinit_tipus_compatibles(Tipus, Tipus); }));
  AFIRMA(1, ({ def_tipus estructura {enter a;} Tipus; __predefinit_tipus_compatibles(Tipus, const Tipus); }));

  AFIRMA(1, ({ estructura {enter a; enter b;} valor; __predefinit_tipus_compatibles(tipus_de(valor.a), tipus_de(valor.b)); }));

  imprimeix("OK\n");
  retorna 0;
}
