#inclou "test.cç"
#inclou <est_def.cç>

def_tipus estructura {
  enter a;
  caràcter b;
  enter c;
  doble d;
} E;

enter principal() {
  AFIRMA(0, desplaçament_de(E, a));
  AFIRMA(4, desplaçament_de(E, b));
  AFIRMA(8, desplaçament_de(E, c));
  AFIRMA(16, desplaçament_de(E, d));

  imprimeix("OK\n");
  retorna 0;
}
